{
  "name": "fusing-angular",
  "description": "fusing-angular",
  "author": "Patrick Michalina",
  "license": "MIT",
  "version": "0.0.0",
  "main": "dist/electron/electron.js",
  "repository": {
    "url": "https://github.com/patrickmichalina/fusing-angular"
  },
  "scripts": {
    "test": "jest",
    "test.watch": "jest --watch",
    "start": "NODE_OPTIONS=--no-warnings ts-node --transpile-only -r dotenv/config fuse build.dev --watch --serve",
    "start.spa": "ts-node -r dotenv/config fuse build.dev --watch --serve --universal=0",
    "start.desktop": "NODE_OPTIONS=--no-warnings ts-node -r dotenv/config fuse build.dev --watch --serve --electron",
    "start.prod": "NODE_OPTIONS=--no-warnings ts-node -r dotenv/config fuse build.prod --prod --serve",
    "start.prod.desktop": "NODE_OPTIONS=--no-warnings ts-node -r dotenv/config fuse build.prod --prod --serve --electron",
    "build.prod": "ts-node fuse build.prod --prod",
    "build.prod.desktop": "ts-node fuse build.prod --prod --electron",
    "heroku-postbuild": "tslint -p tsconfig.json && jest && ts-node fuse build.prod --prod",
    "lint": "tslint -p tsconfig.json"
  },
  "engines": {
    "node": "= 12.3.1"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.800.1",
    "@angular/animations": "^8.1.0-beta.0",
    "@angular/common": "^8.1.0-beta.0",
    "@angular/compiler": "^8.1.0-beta.0",
    "@angular/compiler-cli": "^8.1.0-beta.0",
    "@angular/core": "^8.1.0-beta.0",
    "@angular/forms": "^8.1.0-beta.0",
    "@angular/http": "^8.0.0-beta.10",
    "@angular/language-service": "^8.1.0-beta.0",
    "@angular/platform-browser": "^8.1.0-beta.0",
    "@angular/platform-browser-dynamic": "^8.1.0-beta.0",
    "@angular/router": "^8.1.0-beta.0",
    "@angular/service-worker": "^8.1.0-beta.0",
    "@flosportsinc/ng-env-transfer-state": "^7.0.0",
    "@nguniversal/common": "^8.0.0-rc.1",
    "@nguniversal/express-engine": "^8.0.0-rc.1",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "@types/throng": "^4.0.2",
    "@types/yargs": "^13.0.0",
    "codelyzer": "^5.1.0",
    "cookie-parser": "1.4.4",
    "core-js": "^3.1.3",
    "dotenv": "^8.0.0",
    "electron": "^5.0.2",
    "fuse-box": "^3.7.1",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.1",
    "rxjs": "^6.5.2",
    "terser": "^4.0.0",
    "throng": "^4.0.0",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "tslint-immutable": "^5.5.2",
    "typescript": "^3.5.1",
    "typescript-monads": "^3.13.0",
    "yargs": "^13.2.4",
    "zone.js": "^0.9.1"
  },
  "dependencies": {
    "@angular/platform-server": "^8.1.0-beta.0",
    "compression": "git://github.com/patrickmichalina/compression.git",
    "express": "^4.17.1",
    "express-static-gzip": "^1.1.3"
  },
  "jest": {
    "rootDir": "src",
    "preset": "jest-preset-angular",
    "testPathIgnorePatterns": ["node_modules", ".ngc"],
    "setupFilesAfterEnv": ["../tools/scripts/jest-setup.ts"],
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.json"
      }
    }
  }
}
